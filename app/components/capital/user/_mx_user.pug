mixin user()
  .user
    -
      const _USER_LINKS = [
        ['cabinet', 'Личный кабинет'],
        ['card', 'Активировать карту'],
      ];
    .user__grid
      each item, index in _USER_LINKS
        - const [modifier, text] = item;
        div(class=setModifiers('user__nest', [index + 1]))
          a(class=setModifiers('user__link', [modifier]), href='#')
            span.user__string= text
